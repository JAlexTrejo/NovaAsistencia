# Staging Environment Variables for Nova HR
# WARNING: This file contains sensitive information and should NOT be committed to version control

# Supabase Configuration
VITE_SUPABASE_URL=https://your-staging-project.supabase.co
VITE_SUPABASE_ANON_KEY=your_staging_anon_key_here

# Feature Flags for Staging
VITE_FLAG_BRAND_GLOBAL=1
VITE_FLAG_NEW_RLS=1
VITE_FLAG_PRODUCTION_MONITORING=1
VITE_FLAG_ENHANCED_SECURITY=1
VITE_FLAG_ERROR_TRACKING=1
VITE_FLAG_PERFORMANCE_MONITORING=1
VITE_FLAG_HEALTH_CHECKS=1
VITE_FLAG_MOCK_DATA_DETECTION=1
VITE_FLAG_DEBUG_MODE=1

# Environment
VITE_ENVIRONMENT=staging

# Performance Monitoring
VITE_PERFORMANCE_BUDGET_JS=250000
VITE_PERFORMANCE_BUDGET_CSS=100000

# Security
VITE_SECURITY_HEADERS_ENABLED=1

# Analytics (Staging)
VITE_GA_MEASUREMENT_ID=
VITE_ANALYTICS_ENABLED=0